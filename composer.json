{
    "name": "bfg/bless_model",
    "type": "bfg-app",
    "description": "Recursive auto save eloquent Models",
    "keywords": [
        "laravel",
        "recursive",
        "save",
        "update",
        "bfg",
        "eloquent",
        "models",
        "model"
    ],
    "homepage": "https://github.com/bfg/bless_model",
    "license": "MIT",
    "authors": [
        {
            "name": "bfg",
            "email": "bfg.script@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "bfg/text": "*",
        "bfg/object": "*",
        "illuminate/contracts": "^8.0|^9.0"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "Bfg\\BlessModel\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {

        }
    },
    "scripts": {

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\BlessModel\\ServiceProvider"
            ],
            "aliases": {
                "BlessModel": "Bfg\\BlessModel\\Facades\\BlessModel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
